                "color":"0xffffffff",
                "treeForm":["1", "3", "2", "1", "0"]
            }, {
                "titlePrefixSuffix":["", ""],
                "stageLength":"180",
                "cutProducts":["branch", "root", "log"],
                "atlasXY":["0", "1"],
                "color":"0xffffffff",
                "treeForm":["2", "5", "2", "1", "5"]
            }, {
                "titlePrefixSuffix":["old", ""],
                "stageLength":"240",
                "cutProducts":["branch", "root", "log"],
